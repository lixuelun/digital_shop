<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="struts" extends="struts-default" namespace="/">
		<action name="search" class="searchProduct">
			<result name="success">/show_product.jsp</result>
		</action>
		
		<action name="product" class="product">
			<result name="success">/product.jsp</result>
			<result name="addSuccess">/addShoppingCart_success.jsp</result>
			<result name="addFalse">/addShoppingCart_false.jsp</result>
		</action>
		
		<action name="member" class="member">
			<result name="success">/member_information.jsp</result>
		</action>
	
		<action name="shoppingCart" class="shoppingCart">
			<result name="success">/shoppingCart.jsp</result>
		</action>
	</package>
</struts>    
